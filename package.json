{
  "name": "harry-portfolio",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx --fix",
    "format": "prettier --write src/**/*.{ts,tsx,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,css,md}",
    "format:md": "prettier --write \"*.md\" \"**/*.md\"",
    "format:md:check": "prettier --check \"*.md\" \"**/*.md\"",
    "lint:md": "markdownlint-cli2 \"*.md\" \"**/*.md\"",
    "lint:md:fix": "markdownlint-cli2 --fix \"*.md\" \"**/*.md\"",
    "spell-check": "cspell \"src/**/*.{ts,tsx,css}\" \"*.md\"",
    "spell-check:fix": "cspell \"src/**/*.{ts,tsx,css}\" \"*.md\" --show-suggestions"
  },
  "devDependencies": {
    "@cspell/dict-en_us": "^4.4.18",
    "@cspell/dict-pt-br": "^2.4.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/scrollreveal": "^0.0.9",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "autoprefixer": "^10.4.18",
    "cspell": "^9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.15",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react-hooks": "^5.0.0",
    "markdownlint-cli2": "^0.16.0",
    "postcss": "^8.4.35",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "next": "^14.2.15",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "scrollreveal": "^4.0.9"
  }
}
