{
  "name": "harry-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://Schlorke.github.io/harry-portfolio/",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "lint": "eslint src/**/*.js --fix",
    "lint:fix": "eslint src/**/*.js --fix",
    "lint:check": "eslint src/**/*.js",
    "format": "prettier --write src/**/*.{js,css,html,md}",
    "format:check": "prettier --check src/**/*.{js,css,html,md}",
    "format:md": "prettier --write *.md",
    "format:js": "prettier --write src/**/*.js",
    "format:css": "prettier --write src/**/*.css",
    "format:html": "prettier --write src/**/*.html",
    "spell-check": "cspell \"src/**/*.{html,js,css}\" \"*.md\"",
    "spell-check:fix": "cspell \"src/**/*.{html,js,css}\" \"*.md\" --show-suggestions"
  },
  "devDependencies": {
    "@cspell/dict-en_us": "^4.4.18",
    "@cspell/dict-pt-br": "^2.4.2",
    "cspell": "^9.2.1",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "gh-pages": "^6.3.0",
    "prettier": "^3.5.3",
    "vite": "^7.1.3",
    "vite-plugin-purgecss": "^0.2.13"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "scrollreveal": "^4.0.9"
  }
}
